{
  "name": "merchant-backend",
  "version": "1.0.0",
  "description": "Backend for Merchant Portal",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "db:init": "node db/init.js",
    "db:init:booking": "node BookingDb/init_booking.js",
    "db:setup": "npm run db:init && npm run db:init:booking",
    "db:check": "node scripts/check-database.js",
    "db:fix": "node scripts/fix-orphaned-bookings.js",
    "setup": "npm install && npm run db:setup"
  },
  "dependencies": {
    "better-sqlite3": "^11.0.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1"
  }
}